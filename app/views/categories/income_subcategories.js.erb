$(".sub_category.income_category").html("<%=j(render 'records/subcategories', subcategories: @subcategories) %> ");


$("tbody.sub_category.income_category tr td").bind("click", function(event) {
  if ($(this).data('subcategory-id')) {
    $("tbody.sub_category.income_category tr td.warning").attr("class", "");
    $(this).attr("class", "warning");
    return $('#record_sub_category').val($(this).data('subcategory-id'));
  }
});

$('#record_sub_category').val('<%= @subcategories[0][0].hash_key %>');