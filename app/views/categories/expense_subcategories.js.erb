$(".subcategory.expense_category").html("<%=j(render 'records/subcategories', subcategories: @subcategories) %> ");

$("tbody.subcategory.expense_category tr td").bind("click", function(event) {
  if ($(this).data('subcategory-id')) {
    $("tbody.subcategory.expense_category tr td.warning").attr("class", "");
    $(this).attr("class", "warning");
    $('#record_subcategory').val($(this).data('subcategory-id'));
  }
});

$('#record_subcategory').val('<%= @subcategories[0][0].hash_key %>');